@model IEnumerable<CheckMyCode.Web.ViewModels.Home.IndexProjectsViewModel>
@{
    ViewBag.Title = "Home Page";
}

<div class="container">
    @foreach (var project in Model)
    {
        <div class="row">
            <div class="col-md-10">
                <div class="row">
                    <h4>
                        <a href="#">@project.Name</a>
                    </h4>
                </div>
            </div>
        </div>
        <hr />
    }
</div>